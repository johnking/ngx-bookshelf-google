{
  "name": "ngx-bookshelf-google",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "ng build --prod",
    "ci": "npm run lint && npm run test && npm run docs",
    "clean:all": "npm run clean:tmp && rimraf node_modules",
    "clean:tmp": "rimraf coverage dist tmp docs",
    "cm": "git-cz",
    "docs": "compodoc -p tsconfig.json -d docs",
    "e2e": "ng e2e",
    "e2e:watch": "watch 'ng e2e -s true' src e2e --wait=1",
    "lint": "ng lint",
    "ng": "ng",
    "precommit": "npm run ci",
    "semantic-release": "semantic-release",
    "start": "ng serve",
    "test": "ng test --single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/johnking/ngx-bookshelf-google.git"
  },
  "dependencies": {
    "@angular/animations": "5.2.0",
    "@angular/cdk": "5.0.4",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/material": "5.0.4",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "core-js": "^2.4.1",
    "hammerjs": "2.0.8",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.6.3",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@compodoc/compodoc": "1.0.5",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "semantic-release": "11.0.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2",
    "watch": "1.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
